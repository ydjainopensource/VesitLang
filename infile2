graph  {
    from a to b
    from a to c
    from a to d
    from d to e
    from e to f
 }
bfs a {
    bfsQueuedNodeColor=khaki
    bfsQueuedNodeShape=square
    bfsOutImageDir=imagesBfs/
    bfsPptName=c.pdf
}
dfs a {
    dfsStackNodeColor=khaki
    dfsStackNodeShape=square
    dfsOutImageDir=imagesDfs/
    dfsPptName=d.pdf
}

